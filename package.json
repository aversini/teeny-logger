{
  "name": "teeny-logger",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Arno Versini",
  "description": "A tiny console logger for nodejs CLI apps",
  "homepage": "https://github.com/aversini/teeny-logger",
  "repository": {
    "type": "git",
    "url": "git@github.com:aversini/teeny-logger.git"
  },
  "main": "src/index.js",
  "files": [
    "src/*.js"
  ],
  "scripts": {
    "bump": "bump-and-release -t bump",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "latest": "rimraf LATEST.md && conventional-changelog -o LATEST.md -n \"./configuration/latest/config.js\"",
    "lint:fix": "eslint --fix --color \"./src/**/*.js\"",
    "lint": "eslint --color \"./src/**/*.js\"",
    "prettier:fix": "prettier --loglevel error --write \"src/**/*.js\"",
    "prettier:all": "npm-run-all --serial prettier:fix lint:fix",
    "release": "bump-and-release -t release",
    "test": "cross-env NODE_ENV=\"test\" jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/cli": "^11.0.0",
    "@testing-library/jest-dom": "5.11.9",
    "babel-eslint": "^10.1.0",
    "bump-and-release": "^1.2.0",
    "compare-func": "2.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^7.2.0",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "jest": "26.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "q": "1.5.1",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "kleur": "4.1.4"
  }
}
